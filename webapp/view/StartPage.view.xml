<mvc:View
  controllerName="local.experiment.controller.StartPage"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:core="sap.ui.core"
  height="100%"
>
  <Page
    showHeader="true"
    title="{i18n>appTitle}"
    showFooter="false"
    showNavButton="false"
    backgroundDesign="Solid"
  >
    <content>
      <List items="{/EquipmentSet}">
        <StandardListItem title="{EquipmentNumber}" info="{Description}" />
      </List>
    </content>
  </Page>
</mvc:View>